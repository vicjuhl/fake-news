import simple_model as sm
import pa_classifier as pac
import pandas as pd 

# import csv and transform to csv 

models = {'simple_model': sm, 'pa_classifier': pac}

def infer(model_name: str, df: pd.DataFrame) -> pd.DataFrame:
    models[model_name].infer(df)    # only ideas wouldnt work right now, should models be objects with methods?? 
    return df
# import validation csv

def find_acc(pred_name : str , df : pd.DataFrame) -> float:
    return sum(df['type'] == df['pred_name'])/ len(df)
    
    